{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "outDir": "../dist/",
    "sourceMap": true,
    "target": "ES2015",
    "lib": ["ES2015", "DOM", "DOM.Iterable", "ESNext"],
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node"
  }
}
